{
    "name": "og-image",	 
    "alias": "og-image.now.sh",
    "version": 2,
    "builds": [
        { "src": "public/*", "use": "@now/static" },
        { "src": "card.ts", "use": "@now/node@canary", "config": { "maxLambdaSize": "40mb" } }
    ],
    "routes": [
        { "src": "/", "dest": "/public/index.html" },
        { "src": "/favicon.ico", "dest": "/public/favicon.ico" },
        { "src": "/(.+)", "dest": "/card.ts" }
    ]
}